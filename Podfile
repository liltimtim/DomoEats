# Uncomment this line to define a global platform for your project
 platform :ios, '8.0'
# Uncomment this line if you're using Swift
 use_frameworks!

def install
    pod 'Parse'
    pod 'TDOAuth'
    pod 'SwiftyJSON'
    pod 'SDWebImage'
end

target 'DomoEats' do
    install
end

target 'DomoEatsTests' do
    install
end

target 'DomoEatsUITests' do
    install
end

